{
  "version": 2,
  "name": "futbolargentino-api",
  "builds": [
     { "src": "app.js", "use": "@vercel/node" }
  ],
  "routes": [
     { "src": "/(.*)", "dest": "/app.js" }
  ],
   "functions": {
     "api/*.js": {
       "maxDuration": 30 // This function can run for a maximum of 30 seconds
     }
   }
}